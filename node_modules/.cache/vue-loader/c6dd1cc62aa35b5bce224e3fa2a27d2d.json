rListjob\" :key=\"item.list_id\">\n          <td>{{ item.list_id }}</td>\n          <td>{{ item.date }}</td>\n          <td>{{ item.customer }}</td>\n          <td>{{ item.driver }}</td>\n          <td>{{ item.trucks }}</td>\n          <td>{{ item.source }}</td>\n          <td>{{ item.destination }}</td>\n          <td>{{ item.price }}</td>\n          <td>{{ item.payment }}</td>\n        </tr>\n      </tbody>\n    </template>\n  </v-simple-table>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n  data () {\n    return {\n      trListjob: []\n    }\n  },\n  mounted () {\n    axios.get('http://localhost:3000/getlistjob')\n      .then((response) => {\n        console.log(response.data)\n        this.trListjob = response.data\n        this.source_destination = response.data\n      })\n      .catch((error) => {\n        console.log(error)\n      })\n  }\n}\n</script>\n"]}]}